<keymap version="1" name="GNOME copy" parent="Default for GNOME">
  <action id="Back">
    <keyboard-shortcut first-keystroke="shift alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl alt minus" />
  </action>
  <action id="CollapseRegionRecursively">
    <keyboard-shortcut first-keystroke="ctrl alt subtract" />
  </action>
  <action id="ExpandRegionRecursively">
    <keyboard-shortcut first-keystroke="ctrl alt add" />
  </action>
  <action id="FileStructurePopup">
    <keyboard-shortcut first-keystroke="ctrl f12" />
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="shift alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="ctrl alt equals" />
  </action>
  <action id="IntroduceField" />
  <action id="ShowFilterPopup" />
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="ctrl alt f" />
  </action>
  <action id="ToggleZenMode">
    <keyboard-shortcut first-keystroke="alt f" />
  </action>
  <action id="UsageGrouping.FileStructure" />
</keymap>